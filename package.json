{
  "name": "kakunin",
  "version": "2.2.0",
  "description": "End-to-end testing framework",
  "homepage": "https://thesoftwarehouse.github.io/Kakunin/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheSoftwareHouse/Kakunin.git"
  },
  "author": {
    "name": "The Software House",
    "url": "http://tsh.io"
  },
  "contributors": [
    {
      "name": "Adam Polak"
    },
    {
      "name": "Mariusz Richtscheid"
    },
    {
      "name": "Tomasz Górski"
    },
    {
      "name": "Jakub Paluch"
    },
    {
      "name": "Szymon Stanisz"
    },
    {
      "name": "Wojciech Wójcik"
    }
  ],
  "bin": {
    "kakunin": "./dist/cli.js"
  },
  "main": "./dist/index",
  "scripts": {
    "postinstall": "webdriver-manager update",
    "units": "cross-env NODE_ENV=test mocha --require ./src/tests/init.js --require babel-register \"./src/**/*.spec.js\"",
    "functional": "cd functional-tests && npm i && npm run test",
    "test": "npm run units && npm run functional",
    "build": "babel src --out-dir dist --ignore 'tests','*.spec.js' && babel templates --out-dir dist/templates --copy-files"
  },
  "engines": {
    "node": ">=7.7.3",
    "npm": ">=4.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.0",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "7.4.0",
    "fetch-mock": "^5.11.0",
    "mocha": "^3.4.1"
  },
  "dependencies": {
    "browsermob-proxy": "^1.0.9",
    "moment": "^2.18.1",
    "node-env-file": "^0.1.8",
    "node-fetch": "^1.6.3",
    "node-xlsx": "^0.11.0",
    "path": "^0.12.7",
    "protractor": "5.1.2",
    "protractor-cucumber-framework": "4.0.8",
    "sugar-date": "^2.0.4",
    "inquirer": "^3.0.6",
    "jasmine": "^2.5.2",
    "jasmine-reporters": "^2.2.0",
    "jasmine-spec-reporter": "^4.1.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "extend": "^3.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.3.0",
    "chalk": "^2.1.0",
    "child_process": "^1.0.2",
    "cucumber": "^3.0.3",
    "webdriver-manager": "^12.0.6",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.2.3"
  }
}
