{
  "name": "kakunin",
  "version": "3.1.0-3",
  "description": "End-to-end testing framework",
  "homepage": "https://thesoftwarehouse.github.io/Kakunin/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheSoftwareHouse/Kakunin.git"
  },
  "author": {
    "name": "The Software House",
    "url": "http://tsh.io"
  },
  "contributors": [
    {
      "name": "Adam Polak"
    },
    {
      "name": "Mariusz Richtscheid"
    },
    {
      "name": "Tomasz Górski"
    },
    {
      "name": "Jakub Paluch"
    },
    {
      "name": "Szymon Stanisz"
    },
    {
      "name": "Wojciech Wójcik"
    },
    {
      "name": "Józef Szymala"
    },
    {
      "name": "Adam Nowrot"
    }
  ],
  "bin": {
    "kakunin": "./dist/cli.js"
  },
  "main": "./dist/index",
  "scripts": {
    "units": "cross-env jest  --setupFiles ./src/tests/init.ts --no-cache",
    "functional": "npm run build && cd functional-tests && rm -rf package-lock.json && rm -rf node_modules && npm i && npm run test",
    "test": "npm run units && npm run functional",
    "test-ci": "npm run units && npm run build && cd functional-tests && npm i && npm run test-ci",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint-fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "prettier": "prettier --write 'src/**/*.ts' && prettier --write 'functional-tests/**/*.js",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "prepublishOnly": "npm run build && npm run test"
  },
  "engines": {
    "node": ">=7.7.3",
    "npm": ">=4.1.2"
  },
  "devDependencies": {
    "@types/cucumber": "4.0.7",
    "@types/extend": "3.0.1",
    "@types/faker": "4.1.7",
    "@types/glob": "7.1.1",
    "@types/inquirer": "6.5.0",
    "@types/jest": "24.0.21",
    "@types/lodash": "4.14.144",
    "@types/minimist": "1.2.0",
    "@types/mkdirp": "0.5.2",
    "@types/moment": "2.13.0",
    "@types/node": "12.12.5",
    "@types/node-fetch": "2.5.3",
    "@types/protractor": "4.0.0",
    "@types/shelljs": "0.8.6",
    "@types/hapi__joi": "16.0.3",
    "fetch-mock": "8.0.0-alpha.14",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "ts-jest": "24.1.0",
    "tslint": "6.0.0-beta0",
    "tslint-config-prettier": "1.18.0",
    "tslint-sonarts": "1.9.0",
    "typescript": "3.6.4"
  },
  "dependencies": {
    "@hapi/joi": "16.1.8",
    "ajv": "6.10.2",
    "browsermob-proxy": "1.0.10",
    "browserstack-local": "1.3.7",
    "chai": "4.2.0",
    "chalk": "2.4.2",
    "child_process": "1.0.2",
    "cross-env": "6.0.3",
    "cucumber": "5.1.0",
    "extend": "3.0.2",
    "faker": "4.1.0",
    "glob": "7.1.5",
    "inquirer": "7.0.0",
    "jasmine": "3.5.0",
    "jasmine-reporters": "2.3.2",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "24.9.0",
    "lodash": "4.17.15",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "moment": "2.24.0",
    "node-env-file": "0.1.8",
    "node-fetch": "2.6.0",
    "node-xlsx": "0.15.0",
    "path": "0.12.7",
    "protractor": "5.4.2",
    "protractor-cucumber-framework": "6.1.3",
    "protractor-image-comparison": "3.7.0",
    "protractor-multiple-cucumber-html-reporter-plugin": "1.8.1",
    "shelljs": "0.8.3",
    "sugar-date": "2.0.6",
    "uuid": "3.3.3",
    "webdriver-manager": "13.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/index.ts",
      "!src/**/*.d.ts"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
