{
  "name": "kakunin",
  "version": "v1.16.4",
  "description": "End-to-end testing framework",
  "author": {
    "name": "The Software House",
    "url": "http://tsh.io"
  },
  "contributors": [
    {
      "name": "Adam Polak"
    },
    {
      "name": "Mariusz Richtscheid"
    },
    {
      "name": "Tomasz Gorski"
    },
    {
      "name": "Jakub Paluch"
    },
    {
      "name": "Szymon Stanisz"
    },
    {
      "name": "Wojciech Wojcik"
    }
  ],
  "bin": {
    "kakunin": "./dist/cli.js"
  },
  "main": "./dist/index",
  "scripts": {
    "postinstall": "webdriver-manager update",
    "units": "NODE_ENV=test mocha --require ./src/tests/init.js --require babel-register $(find src -name '*.spec.js')",
    "functional": "cd example && npm i && npm run test",
    "test": "npm run units && npm run functional",
    "build": "babel src --out-dir dist --ignore 'tests','*.spec.js' && babel templates --out-dir dist/templates --copy-files"
  },
  "engines": {
    "node": ">=7.7.3",
    "npm": ">=4.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.0",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "7.0.1",
    "fetch-mock": "^5.11.0",
    "mocha": "^3.4.1"
  },
  "dependencies": {
    "moment": "^2.18.1",
    "node-env-file": "^0.1.8",
    "node-fetch": "^1.6.3",
    "node-xlsx": "^0.7.4",
    "path": "^0.12.7",
    "protractor": "5.1.2",
    "protractor-cucumber-framework": "3.1.2",
    "sugar-date": "^2.0.4",
    "inquirer": "^3.0.6",
    "jasmine": "^2.5.2",
    "jasmine-reporters": "^2.2.0",
    "jasmine-spec-reporter": "^4.1.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "extend": "^3.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-string": "^1.3.0",
    "chalk": "^1.1.3",
    "child_process": "^1.0.2",
    "cucumber": "^2.2.0",
    "cucumber-html-report": "^0.6.0",
    "webdriver-manager": "^12.0.6"
  }
}
